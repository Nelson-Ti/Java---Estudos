       Questão 3
       
 Public class Bolo{
        //atributos
        
 Private String Sabor;
 Private String Cobertura;
 Private preço Float Preco;
 
 
 Construtor não padrão 
 public Bolo()
 {}
 
       //construtor não padrão
 
 Public Bolo(String sabor, String cobertura, float preco)
 {
 this.sabor = sabor;
 this.cobertura = cobertura;
 this.preco = preco;
 }
 
      //metodo de acesso
      
public string get sabor()
{ return sabor;}

//metodo modificador
  public void setsabor(string sabor)
{
      this.sabor = sabor;
 }
 } //fim
 

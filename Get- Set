//main
package test;

public class TestandoGeteSet {

    public static void main(String[] args) {
       
    	Carro c1 = new Carro("COROLA", "Preto", 1424, 2018, 28000);
        c1.status();
      
        Carro c2 = new Carro("GOL", "vermelho", 1782, 2017, 19000);
        c2.status();

        Carro c3 = new Carro("FIESTA", "cinza", 1269, 2019, 23000);
        c3.status();
 
    }
    
}
